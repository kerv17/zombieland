# class member variables go here, for example:
var spawn = Vector2(0,0)
var time = 0
var health
var bar
var score = 0
var a =(get_child_count()-1)
var b = 0.0
var c = Vector2(0,0)
var ms_zombie = 240
func _ready():
	# Called every time the node is added to the scene.
	set_process(true)
	pass
func _process(delta):
	
	#print($enemy.get_child_count(),\",\",Performance.get_monitor(Performance.TIME_FPS))
	pass

func _on_Timer_timeout():
	#if get_node(\"enemy\").get_child_count() <= 15:
	if Performance.get_monitor(Performance.TIME_FPS) >= 60:
		if time >1.0:
			spawn_time()
		spawn(\"res://Spawn.tscn\")
	pass # replace with function body

func spawn(variant):
	randomize()
	spawn.x =rand_range(235,1824)
	randomize()
	spawn.y =rand_range(160,1344)
	var zombie = load(variant).instance()
	zombie.position = spawn
	zombie.ms_zombie = ms_zombie
	get_node(\"enemy\").add_child(zombie)
	

func spawn_time():
	time = get_node(\"Timer\").get_wait_time()
	time -= 0.1
	get_node(\"Timer\").set_wait_time(time)
	get_node(\"Timer\").start()
"

